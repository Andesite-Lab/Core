{
  "ProjectType": "API",
  "Config": {
    "BaseSourceDir": "Source",
    "EntryPoint": "Source/App.ts",
    "OutputDir": "Build",
    "PathAlias": "@/"
  },
  "Infrastructure": {
    "Database": null,
    "RedPanda": null,
    "DragonFly": null
  },
  "Server": {
    "Host": "${HOST}",
    "Port": "${PORT}",
    "BaseUrl": "${BASE_URL}",
    "Logger": true,
    "Http2": false,
    "Security": {
      "AllowedIPs": "${ALLOWED_IPS}",
      "BlockedIPs": "${BLOCKED_IPS}",
      "AllowedOrigins": "${ALLOWED_ORIGINS}"
    }
  }
}