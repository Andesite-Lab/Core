{
  "$schema": "https://json.schemastore.org/package",
  "name": "@andesite-lab/andesite-core",
  "description": "Andesite - Core provides a set of tools to help you build a microservice",
  "version": "1.39.0",
  "type": "module",
  "main": "build/lib.js",
  "types": "build/lib.d.ts",
  "scripts": {
    "dev": "bun --watch source/lib.ts",
    "build": "NODE_ENV=production bun bundler.config.ts && tsc && tsc-alias",
    "start": "NODE_ENV=production bun build/index.js",
    "test": "bun test",
    "docs": "typedoc",
    "fix-lint": "eslint --fix ./source",
    "lint": "eslint ./source"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andesite-Lab/Core.git"
  },
  "bugs": {
    "url": "https://github.com/Andesite-Lab/Core/issues"
  },
  "homepage": "https://github.com/Andesite-Lab/Core#readme",
  "keywords": [
    "andesite",
    "andesite-lab",
    "andesite-core",
    "core"
  ],
  "author": "Ruby",
  "license": "MIT",
  "dependencies": {
    "@azure/storage-blob": "^12.25.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/swagger": "^9.2.0",
    "@scalar/fastify-api-reference": "1.25.58",
    "@vinejs/vine": "^2.1.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "fast-querystring": "^1.1.2",
    "fastify": "^5.1.0",
    "fastify-print-routes": "^4.0.0",
    "fluent-json-schema": "^5.0.0",
    "i18next": "^23.16.4",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@types/bun": "^1.1.13",
    "@types/node": "^22.9.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "mssql": "^11.0.1",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.26.11",
    "typescript-eslint": "^8.13.0"
  },
  "peerDependencies": {
    "typescript": "^5.6.3",
    "@basalt-lab/basalt-helper": "^1.8.1",
    "@basalt-lab/basalt-logger": "^1.9.1"
  }
}